@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("UserLayoutHeadViewComponentPartial")

<body>
    <div class="container-scroller d-flex">

        @await Component.InvokeAsync("UserLayoutSidebarViewComponentPartial")

        <div class="container-fluid page-body-wrapper">

            @await Component.InvokeAsync("UserLayoutNavbarViewComponentPartial")

            @RenderBody()

        </div>
    </div>
    <script src="~/userpaneltemplate/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/userpaneltemplate/js/off-canvas.js"></script>
    <script src="~/userpaneltemplate/js/hoverable-collapse.js"></script>
    <script src="~/userpaneltemplate/js/template.js"></script>
</body>
</html>

